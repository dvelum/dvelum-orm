{
    "name": "dvelum/dvelum-orm",
    "version": "4.0.0-dev",
    "type": "dvelum4-extension",
    "description": "DVelum 4.x ORM",
    "keywords": [
        "dvelum",
        "php",
        "orm"
    ],
    "homepage": "https://github.com/dvelum/dvelum-orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/dvelum-orm/issues",
        "source": "https://github.com/dvelum/dvelum-orm"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php": ">=7.4.0",
        "ext-mysqli": "*",
        "composer/installers": "~1.9",
        "oomphinc/composer-installers-extender": "^2.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master",
        "composer/composer": "^2.0"
    }
}
